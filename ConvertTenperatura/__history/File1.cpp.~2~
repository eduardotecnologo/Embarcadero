#pragma hdrstop
#pragma argsused

#ifdef _WIN32
#include <tchar.h>
#else
  typedef char _TCHAR;
  #define _tmain main
#endif

#include <stdio.h>

int _tmain(int nNumberofArgs, char* paszArgs())
{
    // Insere a temperatura em Celsius
    int celsius;
    cout << "Enter the temperature in Celsius: ";
    cin >> celsius;

	return 0;
}
